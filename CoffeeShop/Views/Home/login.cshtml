
@{
    ViewData["Title"] = "login";
}

<h1>Make like tea, and leaf your personal information in the boxes to register.</h1>
<br />
<br />
<form method="post" action="/home/formreg">

    First Name:
    <input type="text" name="firstname" />
    <br />
    Last Name:
    <input type="text" name="lastname" />
    <br />
    Email:
    <input type="text" name="email" />
    <br />
    Password:
    <input type="password" name="password" />
    <br />
    Birthdate:
    <input type="date" name="birthdate" />
    <br />
    <input type="submit" />

</form>

